<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yxs.shuai.note"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/zmtb"
        android:label="笔记"
        android:theme="@style/AppTheme" >
        <activity
            android:name="yxs.shuai.app.Activity_Login"
            android:windowSoftInputMode="adjustPan"
            android:label="Note" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="yxs.shuai.app.Activity_Main" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_Register"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_UpdatePass"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_SetMB"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_VerifyMB"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_AddNote"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_UpdataNote"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity android:name="yxs.shuai.app.Activity_Setting" >
        </activity>
    </application>

</manifest>